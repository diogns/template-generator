{
  "name": "mantainers-ms",
  "version": "0.10.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "npm start -- --watch",
    "start:debug": "npm start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run test:types && npm run test:unit",
    "test:unit": "jest --passWithNoTests",
    "test:types": "tsc --noEmit",
    "test:unit:watch": "jest --watch",
    "test:unit:cov": "jest --coverage",
    "test:unit:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "aws:login": "aws codeartifact login --tool npm --repository appointment-pod-auna --domain appointments-domain --domain-owner 141351923546 --region us-east-1",
    "prepare": "husky install",
    "migration:create": "typeorm-ts-node-esm migration:create ./src/migrations/$npm_config_name --outputJs",
    "migration:generate": "typeorm-ts-node-esm migration:generate ./src/migrations/$npm_config_name --dataSource ./src/migrationsDataSource.ts --outputJs",
    "migration:run": "typeorm-ts-node-esm migration:run --dataSource ./src/migrationsDataSource.ts",
    "migration:revert": "typeorm-ts-node-esm migration:revert --dataSource ./src/migrationsDataSource.ts",
    "test:unused": "ts-unused-exports ./tsconfig.json",
    "commit": "cz"
  },
  "dependencies": {
    "@auna/common": "^1.0.13",
    "@fastify/express": "^2.0.2",
    "@fastify/helmet": "^10.0.0",
    "@fastify/middie": "^8.0.0",
    "@fastify/static": "^6.5.0",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/cache-manager": "^2.2.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/cqrs": "^9.0.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-fastify": "^9.0.11",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/terminus": "^9.1.1",
    "@nestjs/typeorm": "^9.0.1",
    "@opentelemetry/api": "1.4.0",
    "@opentelemetry/auto-instrumentations-node": "0.35.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "0.35.0",
    "@opentelemetry/id-generator-aws-xray": "1.1.1",
    "@opentelemetry/instrumentation": "0.35.1",
    "@opentelemetry/propagator-aws-xray": "1.1.1",
    "@opentelemetry/resource-detector-aws": "1.2.1",
    "@opentelemetry/resources": "1.9.1",
    "@opentelemetry/sdk-node": "0.35.0",
    "@opentelemetry/sdk-trace-node": "1.9.1",
    "@opentelemetry/semantic-conventions": "1.9.1",
    "@pulumi/auna": "^1.0.40",
    "@pulumi/aws": "^5.13.0",
    "@pulumi/docker": "^3.4.1",
    "@pulumi/policy": "^1.4.0",
    "@pulumi/pulumi": "^3.40.0",
    "aws-jwt-verify": "^3.2.0",
    "aws-sdk": "^2.1213.0",
    "axios": "^1.3.4",
    "cache-manager": "^4.1.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "helmet": "^6.0.1",
    "mysql2": "^2.3.3",
    "neverthrow": "^5.0.0",
    "oracledb": "^5.5.0",
    "passport": "^0.6.0",
    "passport-http-bearer": "^1.0.1",
    "permissions-policy": "^0.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.1",
    "@commitlint/config-conventional": "^18.4.0",
    "@digitalroute/cz-conventional-changelog-for-jira": "^8.0.1",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cache-manager": "^4.0.2",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.4",
    "@types/node": "^16.0.0",
    "@types/passport-http-bearer": "^1.0.37",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "commitizen": "^4.3.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "28.1.2",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "ts-unused-exports": "^10.0.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@digitalroute/cz-conventional-changelog-for-jira",
      "jiraPrefix": "PODAPM",
      "jiraLocation": "post-body"
    }
  }
}
